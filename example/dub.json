	"dependencies": {
		"dlib2": {
            "version": "~dlib2",
            "path": "../"
        }
	},
	"description": "dlib2 example",
	"license": "BSL-1.0",
	"name": "main",
    "importPaths": ["src"],
    "sourcePaths": ["src"],
    "mainSourceFile": "src/main.d",
    "dflags": ["-vgc", "-betterC"],
    "configurations": [
        {
            "name": "application",
            "targetType": "executable",
        },
        {
            "name": "web",
            "targetType": "executable",
            "dflags": ["-mtriple=wasm32-unknown-unknown-wasm", "-link-internally", "-L-allow-undefined", "-L--export-dynamic"],
            "postBuildCommands-windows": ["move main.exe main.wasm"],
            "postBuildCommands-posix": ["mv main main.wasm"]
        }
    ]
}